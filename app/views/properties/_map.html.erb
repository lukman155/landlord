<script async src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDWX-8RTE6uorsBPa_WWkPcZjO3tbbiOdE&callback=initMap"></script>

<div id="map119"></div>

<style>
#map119 {
  width: 100%;
  height: 300px;
}
</style>

<script>
  // Pass latitude and longitude values to JavaScript
  var latitude = <%= @property.latitude %>;
  var longitude = <%= @property.longitude %>;

  function initMap() {
    var map = new google.maps.Map(document.getElementById('map119'), {
      center: { lat: latitude, lng: longitude },
      zoom: 12
    });
    var marker = new google.maps.Marker({
      position: { lat: latitude, lng: longitude },
      map: map
    });
  }
</script>
